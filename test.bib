@ARTICLE{Yabe2012-vj,
  title    = "Mesogenin causes embryonic mesoderm progenitors to differentiate
              during development of zebrafish tail somites",
  author   = "Yabe, Taijiro and Takada, Shinji",
  abstract = "The molecular mechanism underlying somite development differs
